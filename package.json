{
    "name": "@nivinjoseph/n-fra",
    "version": "3.0.1",
    "description": "Infrastructure library",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "packageManager": "yarn@4.0.2",
    "scripts": {
        "ts-compile": "tsc -p .",
        "ts-lint": "eslint . --ext .ts",
        "ts-build": "yarn ts-compile && yarn ts-lint",
        "ts-build-dist": "yarn ts-build && tsc -p ./dist",
        "clean-src": "find ./src -name '*.js' -delete -o -name '*.map' -delete",
        "clean-test": "find ./test -name '*.js' -delete -o -name '*.map' -delete",
        "clean-dist": "find ./dist -name '*.js' -delete -o -name '*.map' -delete -o -name '*.ts' -delete",
        "clean-all": "yarn clean-src && yarn clean-test && yarn clean-dist",
        "test": "yarn ts-build && node --test --enable-source-maps ./test/",
        "publish-package": "yarn ts-build-dist && git add . && git commit -m 'preparing to publish new version' && yarn version patch && git push && yarn npm publish --access=public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nivinjoseph/n-fra.git"
    },
    "keywords": [
        "infrastructure",
        "iac",
        "pulumi"
    ],
    "author": "NiviN",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nivinjoseph/n-fra/issues"
    },
    "homepage": "https://github.com/nivinjoseph/n-fra#readme",
    "peerDependencies": {
        "@pulumi/pulumi": "^3.91.0"
    },
    "devDependencies": {
        "@types/node": "^20.10",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "eslint-plugin-require-extensions": "^0.1.3",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@datadog/datadog-api-client": "^1.18.0",
        "@nivinjoseph/n-defensive": "^2.0.1",
        "@nivinjoseph/n-exception": "^2.0.1",
        "@nivinjoseph/n-ext": "^2.0.1",
        "@nivinjoseph/n-util": "^2.0.1",
        "@pulumi/aws": "5.42.0",
        "@pulumi/awsx": "0.40.1",
        "@pulumi/datadog": "4.23.0",
        "@pulumi/pulumi": "^3.91.0",
        "@pulumi/random": "4.14.0",
        "tslib": "^2.6.2"
    },
    "engineStrict": true,
    "engines": {
        "node": ">=20.10"
    }
}
